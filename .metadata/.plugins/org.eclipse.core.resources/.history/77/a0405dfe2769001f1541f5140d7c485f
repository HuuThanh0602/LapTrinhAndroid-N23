package vidu;

import java.util.Arrays;

public class Test {
	public static void main(String[] args) {
		int [] a = new int[] {1, 8, 2, 6, 4, 3, 7,9};
		int[] b = new int[15];
		
		//
		System.out.println("a ban đầu: "+a.toString());
		
		//Hàm sắp xếp 
		Arrays.sort(a);
		System.out.println("a sau khi sắp xếp: "+a.toString());

		
		//ham tìm kiếm
		System.out.println(Arrays.binarySearch(a, 4)); // đã sắp xếp tăng dần nó rồi xử lý được
		System.out.println(Arrays.binarySearch(a, -1));

		
	}
}
