package bai_04;

public class Fibonacci {

	 public static int Fibonacci(int Max) {
//		 	int fisrtNumber = 0;
//		 	int secondNumber = 1;
//		 	int fibonacci = '\0';
//		 	
//		 	System.out.print(fisrtNumber + " ");
//		 	System.out.print(secondNumber + " ");
//		 	
//		 	for (int i = 2; i < Max; i++) {
//		 		fibonacci = fisrtNumber + secondNumber;
//		 		
//		 		System.out.print(fibonacci + " ");
//				
//		 		fisrtNumber = secondNumber;
//		 		secondNumber = fibonacci;
//			}
		 
		 	
		 	if (Max < 0) {
		 		return -1;
		 	} else if( Max == 0 || Max == 1){
		 		return Max;
		 	}else {
		 		return Fibonacci(Max - 1) + Fibonacci(Max-2);
		 	}
		}
	 
	 
	 public static void main(String[] args) {
		 for (int i = 0; i <100; i++) {
			System.out.print(Fibonacci(i)+" ");
		}
	}
}

