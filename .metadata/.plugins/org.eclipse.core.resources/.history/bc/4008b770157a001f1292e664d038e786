package bai_02;

public class Test {
	public static void main(String[] args) {
		Ngay n1 = new Ngay(15, 2, 2000);
		Ngay n2 = new Ngay(18, 9, 2002);
		Ngay n3 = new Ngay(21, 2, 2000);
		
		GiamDocTaiChinh tc1 = new GiamDocTaiChinh("Nguyễn Thu Hương", n3, 45);
		GiamDocDieuHanh dh1 = new GiamDocDieuHanh("Nguyễn Văn Hữu", n1, 54);
		
		System.out.println("Ngày nhậm chức của CFO: " + tc1.layRaNgayNhanChuc());
		System.out.println("Ngày nhậm chức của CEO: " + dh1.layRaNgayNhanChuc());
		
		System.out.println("Học vị của CFO là: " + tc1.hocVi());
		System.out.println("Học vị của CEO là: " + dh1.hocVi());
		
		System.out.println("Tên giám đốc CFO là: " + tc1.layRaTenGiamDoc());
		System.out.println("Tên giám đốc CEO là: " + dh1.layRaTenGiamDoc());
		
		
		
		
	}

}
